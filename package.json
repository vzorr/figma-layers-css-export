{
  "name": "figma-to-rn-plugin",
  "version": "2.1.0",
  "description": "Enhanced Figma to React Native Plugin with Automatic Theme Generation and Better Error Handling",
  "main": "dist/code.js",
  "scripts": {
    "build": "node scripts/build.js",
    "build:plugin": "webpack --config webpack.config.js --env target=plugin --mode=production",
    "build:ui": "webpack --config webpack.config.js --env target=ui --mode=production",
    "dev": "node scripts/dev.js",
    "dev:plugin": "webpack --config webpack.config.js --env target=plugin --mode=development --watch",
    "dev:ui": "webpack --config webpack.config.js --env target=ui --mode=development --watch",
    "dev:serve": "webpack serve --config webpack.config.js --env target=ui --mode=development",
    "clean": "node scripts/clean.js",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:types": "npm run type-check",
    "analyze": "webpack-bundle-analyzer dist/ui.js",
    "postinstall": "npm run type-check"
  },
  "keywords": [
    "figma",
    "plugin",
    "react-native",
    "design-system",
    "theme-generation",
    "code-generation",
    "responsive-design",
    "typescript",
    "design-tokens",
    "automation"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/figma-to-rn-plugin.git"
  },
  "homepage": "https://github.com/yourusername/figma-to-rn-plugin#readme",
  "bugs": {
    "url": "https://github.com/yourusername/figma-to-rn-plugin/issues"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@figma/eslint-plugin-figma-plugins": "^0.15.0",
    "@figma/plugin-typings": "^1.95.0",
    "@types/figma": "^1.0.4",
    "@types/node": "^20.19.4",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.7.6",
    "rimraf": "^5.0.5",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.10.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Safari versions"
  ],
  "figma": {
    "id": "figma-to-rn-plugin",
    "name": "Figma to React Native",
    "main": "dist/code.js",
    "ui": "dist/ui.html",
    "documentAccess": "dynamic-page",
    "editorType": [
      "figma"
    ],
    "networkAccess": {
      "allowedDomains": [
        "none"
      ]
    }
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "volta": {
    "node": "18.19.0",
    "npm": "10.2.3"
  }
}